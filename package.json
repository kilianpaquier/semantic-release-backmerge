{
  "name": "@kilianpaquier/semantic-release-backmerge",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "version": "1.1.1",
  "packageManager": "bun@1.1.7",
  "files": [
    "CHANGELOG.md",
    "dist/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "semantic-release-plugin",
    "backmerge"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir dist --target node --minify",
    "clean": "git clean -Xf ./*",
    "lint": "eslint --ignore-pattern 'dist/*'",
    "test": "bun test --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/bun": "latest",
    "@types/eslint__js": "^8.42.3",
    "@types/git-url-parse": "^9.0.3",
    "@types/lodash": "^4.17.1",
    "@types/semantic-release__error": "^3.0.3",
    "@types/semver": "^7.5.8",
    "bun": "^1.1.8",
    "eslint": "^9.2.0",
    "globals": "^15.1.0",
    "npm-check-updates": "^16.14.20",
    "typescript-eslint": "^7.8.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "@semantic-release/error": "^4.0.0",
    "aggregate-error": "^5.0.0",
    "execa": "^8.0.1",
    "git-url-parse": "^14.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "semantic-release": "^23.0.8",
    "semver": "^7.6.2"
  }
}
