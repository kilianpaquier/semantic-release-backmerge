{
  "name": "@kilianpaquier/semantic-release-backmerge",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "version": "1.6.13",
  "packageManager": "bun@1.2.9",
  "files": [
    "CHANGELOG.md",
    "dist/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "url": "https://github.com/kilianpaquier/semantic-release-backmerge"
  },
  "keywords": [
    "semantic-release-plugin",
    "backmerge"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir dist --target node --minify",
    "clean": "git clean -Xf ./*",
    "lint": "eslint --ignore-pattern 'dist/*'",
    "cover": "bun test --coverage --coverage-reporter=lcov --coverage-dir=reports"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/bun": "^1.2.9",
    "@types/debug": "^4.1.12",
    "@types/git-up": "^8.1.0",
    "@types/git-url-parse": "^16.0.0",
    "@types/lodash": "^4.17.16",
    "@types/semantic-release__error": "^3.0.3",
    "@types/semver": "^7.7.0",
    "eslint": "^9.24.0",
    "npm-check-updates": "^17.1.18",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "@octokit/core": "^6.1.5",
    "@semantic-release/error": "^4.0.0",
    "aggregate-error": "^5.0.0",
    "debug": "^4.4.0",
    "execa": "^9.5.2",
    "git-up": "^8.1.1",
    "git-url-parse": "^16.0.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "semantic-release": "^24.2.3",
    "semver": "^7.7.1",
    "url-join": "^5.0.0"
  }
}
