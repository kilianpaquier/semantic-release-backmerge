{
  "name": "@kilianpaquier/semantic-release-backmerge",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "version": "1.7.1",
  "packageManager": "bun@1.2.22",
  "files": [
    "CHANGELOG.md",
    "dist/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "url": "https://gitlab.com/kilianpaquier/semantic-release-backmerge"
  },
  "keywords": [
    "semantic-release-plugin",
    "backmerge"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir dist --target node --minify",
    "clean": "git clean -Xf ./*",
    "cover": "bun test --coverage --coverage-reporter=lcov --coverage-dir=reports --reporter=junit --reporter-outfile=reports/node-test.xunit.xml",
    "lint": "eslint --ignore-pattern dist",
    "test": "bun test"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/bun": "1.3.9",
    "@types/debug": "4.1.12",
    "@types/git-up": "8.1.0",
    "@types/git-url-parse": "16.0.2",
    "@types/lodash": "4.17.23",
    "@types/semantic-release__error": "3.0.3",
    "@types/semver": "7.7.1",
    "eslint": "10.0.0",
    "npm-check-updates": "19.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0"
  },
  "dependencies": {
    "@octokit/core": "7.0.6",
    "@semantic-release/error": "4.0.0",
    "aggregate-error": "5.0.0",
    "debug": "4.4.3",
    "execa": "9.6.1",
    "git-up": "8.1.1",
    "git-url-parse": "16.1.0",
    "lodash": "4.17.23",
    "node-fetch": "3.3.2",
    "semantic-release": "25.0.3",
    "semver": "7.7.4",
    "url-join": "5.0.0"
  }
}
